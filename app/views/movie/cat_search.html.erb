<!DOCTYPE html>
<html>
  <head>
    <title>FinalProject</title>
  <body>
    
    <h1> Movie Category </h1>
      <% @movie4.each do |movie| %>
        <h2> <%= movie.name %> </h2>
        <p> <%=  movie.description %> </p>
        <p> <%= image_tag movie.image %></p>
        <p> Release Date: <%= movie.release_date %> </p>
        <p> Price : <%= movie.order_price %> </p>
      <% end %>
  </body>
</html>